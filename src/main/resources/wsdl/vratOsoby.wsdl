<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch0="http://ais/uk/typy"
                  xmlns:sch1="http://ais/uk" xmlns:sch2="http://ais/vratOsoby/reqtypy"
                  xmlns:sch3="http://ais/vratOsoby/typy" xmlns:sch4="http://ais/vratOsoby"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://ais/vratOsoby"
                  targetNamespace="http://ais/vratOsoby">
    <wsdl:types xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ais/uk/typy"
                   attributeFormDefault="unqualified" elementFormDefault="qualified"
                   targetNamespace="http://ais/uk/typy">
            <xs:simpleType name="connection" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Identifikator transakcie
                    </xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
            </xs:simpleType>
            <xs:complexType name="mapIntDec" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:int" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:decimal" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapIntStr" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:int" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapStrDate" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element minOccurs="0" name="value" nillable="true" type="xs:date"
                                xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapStrDec" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:decimal" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapStrInt" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element minOccurs="0" name="value" nillable="true" type="xs:int"
                                xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapStrStr" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapStrLong" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element minOccurs="0" name="value" nillable="true" type="xs:long"
                                xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapLongDec" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:long" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:decimal" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="mapLongStr" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="key" type="xs:long" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="value" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:typy="http://ais/uk"
                   attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://ais/uk">
            <xs:element name="AISFault" type="typy:AISFault" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
            <xs:complexType name="AISFault" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="Code" type="xs:int" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="Message" type="xs:string" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ais/vratOsoby/reqtypy"
                   attributeFormDefault="unqualified" elementFormDefault="qualified"
                   targetNamespace="http://ais/vratOsoby/reqtypy">
            <xs:complexType name="FilterType" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element name="incremental" type="xs:boolean" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="odId" nillable="true" type="xs:int" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                    <xs:element name="doId" nillable="true" type="xs:int" xmlns:xs="http://www.w3.org/2001/XMLSchema"/>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:typy="http://ais/vratOsoby/typy"
                    attributeFormDefault="unqualified" elementFormDefault="qualified"
                    targetNamespace="http://ais/vratOsoby/typy">
            <xsd:complexType name="LZOsoba" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:all xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:element minOccurs="0" name="adresaOsoby" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="LZAdresaOsoby"
                                             type="typy:LZAdresaOsoby" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="cisloPasu" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Číslo pasu
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="cop" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Číslo občianskeho preukazu
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="datumNarodenia" type="xsd:date" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Dátum narodenia
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="email" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Pridelený email
                                univerzitou
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="liveID" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Identifikátor pre Micosoft
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="emailPrivate" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Súkromný email osoby
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="id" type="xsd:int" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Id osoby v AiS2
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="identifKarta" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="LZIdentifKarta"
                                             type="typy:LZIdentifKarta" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="kodNarodnost" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód národnosti podľa
                                číselníka
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodPohlavie" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód pohlavia:
                                M-muž
                                Ž-žena
                                N-nezistené
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodRodinnyStav" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód rodinného stavu podľa
                                číselníka
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodStat" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód štátnej príslušnosti
                                podľa číselníka
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="meno" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Meno
                                osoby(študenta/zamestnanca)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="plneMeno" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Celé meno osoby(aj s
                                titulmi)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="pouzivatel" type="typy:SPPouzivatel"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                    <xsd:element name="povPriezvisko" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Rodné priezvisko osoby
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="priezvisko" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Priezvisko
                                osoby(študenta/zamestnanca)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodTypVzdelania" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód typu dosiahnutého
                                vzdelania podľa číselníka
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="prihlaskaUch" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="PKPrihlaska"
                                             type="typy:PKPrihlaska" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="rodneCislo" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Rodné číslo osoby
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="sklonovanieNar" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="LZSklonMNarod"
                                             type="typy:LZSklonMNarod" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="skratkaAkademickyTitul" type="xsd:string"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Skratka akademického
                                titulu
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="skratkaCestnyTitul" type="xsd:string"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Skratka čestného titulu
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="skratkaHodnost" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Skratka hodnosti
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="skratkaVedPegHodnost" type="xsd:string"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Skratka vedecko-pedagogickej
                                hodnosti
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="skratkaVedeckaHodnost" type="xsd:string"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Skratka vedeckej hodnosti
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="studium" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="ESStudium" type="typy:ESStudium"
                                             xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="telefon" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Telefónne číslo
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="typySuhlasu" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="LZVektorSuhlasu"
                                             type="typy:LZVektorSuhlasu" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="urlFotky" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Url k fotografii uloženej v
                                AiS2
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="zamestnanec" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="LZZamestnanec"
                                             type="typy:LZZamestnanec" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="idCRS" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Id študenta z CRŠ
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodObecNarodCRS" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód miesta narodenia pre
                                CRŠ
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kodStatNarodCRS" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Kód štátu z číselníka CRŠ
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="statNarod" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Názov štátu narodenia CRŠ
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="esi" nillable="true" type="xsd:string"
                                 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">European student
                                identifier
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:all>
            </xsd:complexType>
            <xsd:complexType name="LZAdresaOsoby" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:all xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:element name="PSC" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                    <xsd:element name="adresat" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Pre bližšie určenie adresáta
                                ak je meno a priezvisko nedostatočné. (napr. ak je to iba podnájom tak sa tom uvádza "u
                                pana XXX " alebo nazov firmy)
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="id" type="xsd:int" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                    <xsd:element name="kodStat" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
                    <xsd:element name="kodTypAdresy" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">T - adresa trvalého pobytu
                                K - korešpondečná adresa
                                P - adresa prechodného pobytu
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
